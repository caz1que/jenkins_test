properties([disableConcurrentBuilds()])

pipeline {
  agent {
    label 'master'
  }
  options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timestamps()
  }
  stages {
      stage("Docker Build") {
          steps {
              echo "==== docker build ===="
              dir ('docker') {
                sh 'docker build . '
              }
          }
      }
  }
}